    <head>
        <style>
            div{
                right:30px;
                border:3px solid green;
            }

            div.absolute{
                position: absolute;
                left:50px;
                border: 3px solid blue;
            }

            div.relative{
                position:relative;
                left:100px;
                border:3px solid green;
            }

            div.fixed{
                position: fixed;
                top:0;
                right:20px;
                width:300px;
                border:3px solid green;
            }

            div.sticky{
                position: sticky;
                top:1px;
                left:20px;
                width:100px;
                border:3px solid green;

            }
        </style>   
    </head>     
        <body>
            <!--chang the position to show difference
            static
            relative
            fixed
            absolute
            sticky-->
            <h2>POSITIONING</h2>
            <div>
                div with no position property
            </div>
            <div class="sticky">
                div with stivky property
            </div>    
            <div class="relative">
                this position has relative element
            </div>
            <div class="absolute">    
                Absolute class div
            </div>    
            <div class="fixed">
                div with fixed property
                </div>

